{
  "name": "taro",
  "version": "1.0.0",
  "private": true,
  "description": "easy taro demo",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "git-cz": "cz",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "changelog": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "@tarojs/components": "3.6.11",
    "@tarojs/helper": "3.6.11",
    "@tarojs/plugin-framework-react": "3.6.11",
    "@tarojs/plugin-platform-alipay": "3.6.11",
    "@tarojs/plugin-platform-h5": "3.6.11",
    "@tarojs/plugin-platform-jd": "3.6.11",
    "@tarojs/plugin-platform-qq": "3.6.11",
    "@tarojs/plugin-platform-swan": "3.6.11",
    "@tarojs/plugin-platform-tt": "3.6.11",
    "@tarojs/plugin-platform-weapp": "3.6.11",
    "@tarojs/react": "3.6.11",
    "@tarojs/runtime": "3.6.11",
    "@tarojs/shared": "3.6.11",
    "@tarojs/taro": "3.6.11",
    "axios": "^1.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3.6.11",
    "@tarojs/taro-loader": "3.6.11",
    "@tarojs/test-utils-react": "^0.1.1",
    "@tarojs/webpack5-runner": "3.6.11",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-preset-taro": "3.6.11",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-taro": "3.6.11",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^14.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2",
    "react-refresh": "^0.11.0",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.4.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4",
    "validate-commit-msg": "^2.14.0",
    "webpack": "5.78.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css,less}": [
      "prettier --write",
      "eslint  --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
